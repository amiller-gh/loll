{
  "name": "rebound-api",
  "version": "2.0.0-beta.0",
  "description": "An Express middleware for elegant API creation",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --jsx preserve -p tsconfig.json",
    "pretest": "yarn run build",
    "test": "mocha -G dist/test",
    "watch": "watch 'yarn run test' './src' './test' --wait=1",
    "start": "node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reboundjs/rebound-api.git"
  },
  "keywords": [
    "express",
    "api",
    "REST",
    "middleware",
    "connect"
  ],
  "author": "Adam Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reboundjs/rebound-api/issues"
  },
  "homepage": "https://github.com/reboundjs/rebound-api#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/walk": "^2.3.0",
    "express": "^4.17.1",
    "mocha": "^6.2.2",
    "typescript": "^3.7.4",
    "watch": "^1.0.2"
  }
}
