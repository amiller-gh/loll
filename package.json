  "bugs": {
    "url": "https://github.com/amiller-gh/loll/issues"
  },
  "homepage": "https://github.com/amiller-gh/loll#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node-fetch": "^2.5.4",
    "@types/walk": "^2.3.0",
    "express": "^4.17.1",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.4",
    "watch": "^1.0.2"
